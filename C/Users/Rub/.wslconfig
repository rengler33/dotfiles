[wsl2]
# Default memory is set to consume 80% of system memory
# Uncomment the line below if you're on a weak computer and need to restrict memory usage, adjust as needed
#memory=0.5GB 

# virtual hard disks can grow and have issues releasing memory so you may not want it on your main disk
# https://github.com/microsoft/WSL/issues/4699
# see above link if your disk has already grown and you need options to shrink it
# Uncomment the line below if you'd like to put the virtual hard disk off of C
# swapFile=D:\\wsl2-swap.vhdx 
